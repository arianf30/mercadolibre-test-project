{
  "name": "@meli-project/app-meli",
  "version": "1.0.8",
  "description": "",
  "main": "./src/main/server/server.ts",
  "scripts": {
    "test": "jest --passWithNoTests --silent",
    "test:all": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:int": "npm test -- --watch -c jest-int-config.js",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "build": "npm-run-all --parallel build:*",
    "build:build-server": "esbuild ./src/main/server/server.ts --bundle --outfile=build/server.js --platform=node",
    "build:build-client": "esbuild ./src/client/_app.tsx --bundle --outdir=src/public/dist",
    "start": "node ./build/server.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:build-server": "esbuild ./src/main/server/server.ts --bundle --outfile=build/server.js --platform=node --watch",
    "dev:build-client": "esbuild ./src/client/_app.tsx --bundle --outdir=src/public/dist --watch",
    "dev:start": "nodemon ./build/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/supertest": "^2.0.12",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.2",
    "esbuild": "^0.15.9",
    "esbuild-register": "^3.3.3",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@meli-project/ds.meli": "*",
    "@meli-project/ds.meli-react": "*",
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "fast-glob": "^3.2.12",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^9.4.0",
    "react-router-dom": "^5.3.0"
  }
}
