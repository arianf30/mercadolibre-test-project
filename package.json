{
  "name": "mercadolibre-test-project",
  "version": "1.0.0",
  "description": "",
  "main": "./src/main/server/serverr.ts",
  "scripts": {
    "test": "jest --passWithNoTests --silent",
    "lint": "eslint src/**/*.ts",
    "start": "NODE_ENV=development ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/main/server/server.ts",
    "test:all": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:int": "npm test -- --watch -c jest-int-config.js",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "prepare": "husky install",
    "dev": "npm-run-all --parallel dev:*",
    "dev:build-server": "nodemon --exec \"node -r esbuild-register ./src/main/server/server.ts\" -e .ts",
    "dev:build-client": "node ./src/client/builder.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.2",
    "esbuild": "^0.15.9",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.23.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "fast-glob": "^3.2.12",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^9.4.0",
    "react-router-dom": "^5.3.0"
  }
}
